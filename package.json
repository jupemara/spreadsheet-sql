    "@babel/core": "^7.18.10",
    "@babel/preset-env": "^7.18.10",
    "@babel/preset-typescript": "^7.18.6",
    "@types/jest": "^28.1.7",
    "ava": "^4.3.1",
    "babel-jest": "^28.1.3",
    "jest": "^28.1.3",
    "npm-check-updates": "^16.0.5",
    "prettier": "^2.7.1",
